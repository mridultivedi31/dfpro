{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfprodatafactorymridul"
		},
		"ls_dfpro_sqldb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_dfpro_sqldb'"
		},
		"ls_dfprostorage_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dfprostorage_adls'"
		},
		"ls_dfpro_sqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mridulsserver.database.windows.net"
		},
		"ls_dfpro_sqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mridulsdatabase"
		},
		"ls_dfpro_sqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminadmin"
		},
		"ls_dfprostorage_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dfprostorageadls.dfs.core.windows.net/"
		},
		"ls_restapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfprodatafactorymridul_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dfpro_sqldb_password": {
						"value": "[parameters('ls_dfpro_sqldb_password')]"
					},
					"ls_dfprostorage_adls_accountKey": {
						"value": "[parameters('ls_dfprostorage_adls_accountKey')]"
					},
					"ls_dfpro_sqldb_properties_typeProperties_server": {
						"value": "[parameters('ls_dfpro_sqldb_properties_typeProperties_server')]"
					},
					"ls_dfpro_sqldb_properties_typeProperties_database": {
						"value": "[parameters('ls_dfpro_sqldb_properties_typeProperties_database')]"
					},
					"ls_dfpro_sqldb_properties_typeProperties_userName": {
						"value": "[parameters('ls_dfpro_sqldb_properties_typeProperties_userName')]"
					},
					"ls_dfprostorage_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_dfprostorage_adls_properties_typeProperties_url')]"
					},
					"ls_restapi_properties_typeProperties_url": {
						"value": "[parameters('ls_restapi_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}